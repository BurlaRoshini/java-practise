# java-practise
public class Main{
    public static void main(String[] args){
        int[] a = {1,67,98,90,66,55,234,66};
        int max = a[0];
        for(int i=1;i<a.length;i++){
            if(a[i]>max)
            max=a[i];
            
        }
        System.out.println(max);
        
        a[0]=9999;
        max = a[a.length-1];
        for(int i=a.length-2;i>=0;i--){
            if(a[i]>max)
            max=a[i];
            
        }
        System.out.println(max);
    }
}
